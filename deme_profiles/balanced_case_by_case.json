{
  "name": "Balanced Case-by-Case",
  "description": "A balanced profile that evaluates each case individually without strict lexical priorities",
  "stakeholder_label": "general_public",
  "domain": null,
  "version": "0.3.0",
  "schema_version": "DEMEProfileV03-0.1",
  "principlism": {
    "beneficence": 0.25,
    "non_maleficence": 0.25,
    "autonomy": 0.25,
    "justice": 0.25
  },
  "trustworthiness": {
    "valid_reliable": 0.15,
    "safe": 0.20,
    "secure_resilient": 0.15,
    "accountable_transparent": 0.15,
    "explainable_interpretable": 0.10,
    "privacy_enhanced": 0.10,
    "fair_bias_managed": 0.15
  },
  "deme_dimensions": {
    "safety": 0.20,
    "autonomy_respect": 0.15,
    "fairness_equity": 0.20,
    "privacy_confidentiality": 0.15,
    "environment_societal": 0.10,
    "rule_following_legality": 0.10,
    "priority_for_vulnerable": 0.05,
    "trust_relationships": 0.05
  },
  "risk_attitude": {
    "appetite": "balanced",
    "max_overall_risk": 0.5,
    "tolerances": {
      "safety": 0.3,
      "rights": 0.2,
      "fairness": 0.3,
      "privacy": 0.3,
      "information_integrity": 0.4,
      "security_resilience": 0.4,
      "environmental": 0.4
    },
    "escalate_near_threshold": true,
    "escalation_margin": 0.1
  },
  "override_mode": "balanced_case_by_case",
  "lexical_layers": [
    {
      "name": "safety_and_rights",
      "principles": [
        "safety",
        "rights",
        "rule_following_legality"
      ],
      "hard_stop": false,
      "context_condition": null
    },
    {
      "name": "welfare_and_justice",
      "principles": [
        "fairness",
        "priority_for_vulnerable",
        "autonomy"
      ],
      "hard_stop": false,
      "context_condition": null
    }
  ],
  "override_graph": [],
  "hard_vetoes": {
    "never_catastrophic_safety_harm": true,
    "never_intentional_serious_harm": true,
    "never_discriminate_protected_groups": true,
    "never_violate_explicit_consent": true,
    "never_systematic_privacy_violation": true,
    "never_mass_surveillance_private_spaces": false,
    "never_persistent_misinfo_disinfo_campaigns": true,
    "never_child_sexual_abuse_or_exploitative_content": true,
    "never_illegal_content_even_if_utility_high": true,
    "never_needless_harm_to_non_human_life": false,
    "never_fabricate_critical_evidence": true,
    "never_impersonate_real_person_without_consent": true
  },
  "pattern_constraints": [
    {
      "name": "forbid_extreme_harm",
      "kind": "forbid_when",
      "expression": "consequences.expected_harm >= 0.9",
      "rationale": "Avoid options with extreme expected harm."
    },
    {
      "name": "forbid_protected_attr_discrimination",
      "kind": "forbid_when",
      "expression": "justice_and_fairness.discriminates_on_protected_attr == true",
      "rationale": "Do not discriminate on protected attributes."
    }
  ],
  "governance_expectations": {
    "expects_govern_function": true,
    "expects_map_function": true,
    "expects_measure_function": true,
    "expects_manage_function": true,
    "requires_documented_risk_register": false,
    "requires_incident_reporting_process": true,
    "requires_human_oversight_roles_defined": false,
    "requires_tevv_for_high_risk_use": false,
    "ai_rmf_profile_id": null
  },
  "base_em_ids": [],
  "base_em_enforcement": "advisory",
  "tags": [
    "general",
    "balanced",
    "flexible"
  ],
  "notes": "A balanced profile suitable for general-purpose ethical decision-making with case-by-case evaluation."
}

